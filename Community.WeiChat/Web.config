<configuration>
  <configSections>
    <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net"/>
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false"/>
  </configSections>
  <log4net>
    <!-- RollingFileAppender looks after rolling over files by size or date -->
    <appender name="errorAppender" type="log4net.Appender.RollingFileAppender">
      <filter type="log4net.Filter.LevelMatchFilter">
        <levelToMatch value="ERROR"/>
      </filter>
      <filter type="log4net.Filter.DenyAllFilter"/>
      <File value="C:\log\tnmb.log"/>
      <appendToFile value="true"/>
      <rollingStyle value="Date"/>
      <datePattern value="(yyyyMMdd)"/>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="Time: %d Place：%logger %n Thread: [%t] LogLevel：%-5p Description：%m%n"/>
      </layout>
    </appender>
    <appender name="infoAppender" type="log4net.Appender.RollingFileAppender">
      <filter type="log4net.Filter.LevelMatchFilter">
        <levelToMatch value="INFO"/>
      </filter>
      <filter type="log4net.Filter.DenyAllFilter"/>
      <File value="C:\log\tnmbInfo.log"/>
      <appendToFile value="true"/>
      <rollingStyle value="Date"/>
      <datePattern value="(yyyyMMdd)"/>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="Time: %d Place：%logger %n Thread: [%t] LogLevel：%-5p Description：%m%n"/>
      </layout>
    </appender>
    <appender name="debugAppender" type="log4net.Appender.RollingFileAppender">
      <filter type="log4net.Filter.LevelMatchFilter">
        <levelToMatch value="DEBUG"/>
      </filter>
      <filter type="log4net.Filter.DenyAllFilter"/>
      <File value="C:\log\tnmbDebug.log"/>
      <appendToFile value="true"/>
      <rollingStyle value="Date"/>
      <datePattern value="(yyyyMMdd)"/>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="Time: %d Place：%logger %n Thread: [%t] LogLevel：%-5p Description：%m%n"/>
      </layout>
    </appender>
    <!-- Setup the root category, add the appenders and set the default level -->
    <root>
      <level value="ALL"/>
      <appender-ref ref="errorAppender"/>
      <appender-ref ref="InfoAppender"/>
      <appender-ref ref="debugAppender"/>
    </root>
    <logger name="debugLog" additivity="false">
      <level value="DEBUG"/>
      <appender-ref ref="errorAppender"/>
      <appender-ref ref="InfoAppender"/>
      <appender-ref ref="debugAppender"/>
    </logger>
    <logger name="errorLog" additivity="false">
      <level value="ERROR"/>
      <appender-ref ref="errorAppender"/>
    </logger>
  </log4net>
  <connectionStrings>
    <add name="eduEntities" connectionString="metadata=res://*/EduDb.csdl|res://*/EduDb.ssdl|res://*/EduDb.msl;provider=System.Data.SqlClient;provider connection string=&quot;data source=122.225.101.119;initial catalog=CSAPI;user id=an;password=sa;MultipleActiveResultSets=True;App=EntityFramework&quot;" providerName="System.Data.EntityClient"/>
  </connectionStrings>
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0"/>
    <add key="webpages:Enabled" value="false"/>
    <add key="ClientValidationEnabled" value="true"/>
    <add key="UnobtrusiveJavaScriptEnabled" value="true"/>
  </appSettings>
  <!--
